<UserControl x:Class="DemoViewer.Radar"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:DemoViewer"
             mc:Ignorable="d" 
             d:DesignHeight="1024" d:DesignWidth="1024">
    <Viewbox>
        <Grid x:Name="gridMain">
            <Image x:Name="imageBackground" Stretch="Uniform" Height="1024" Width="1024"/>
            <Canvas x:Name="canvasMain" />
            <StackPanel x:Name="stackKillfeed" Margin="15"/>
        </Grid>
    </Viewbox>
</UserControl>
